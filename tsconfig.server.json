{
  "extends": "./tsconfig.app.json",
  "compilerOptions": {
    "outDir": "./out-tsc/app-server",
    "baseUrl": ".",
    "paths": {
      "@shared/*": ["app/shared/*"],
      "@utils/*": ["app/utils/*"],
      "@apis/*": ["app/core/apis/*.api.ts"],
      "@environment": ["environments/environment.ts"]
    }
  },
  "angularCompilerOptions": {
    "entryModule": "app/app.server.module#AppServerModule"
  }
}
