{
  "name": "ng-zorro-pro",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve -c hmr --port 4220",
    "build": "ng build",
    "build:prod": "ng build --prod --stats-json",
